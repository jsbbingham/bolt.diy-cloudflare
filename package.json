{
  "name": "bolt",
  "version": "0.0.7",
  "description": "An AI Agent",
  "private": true,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "author": {
    "name": "bolt.diy team",
    "email": "maintainers@bolt.diy"
  },
  "scripts": {
    "build": "remix build",
    "dev": "node pre-start.cjs && remix dev",
    "start": "wrangler pages dev . --port 8788",
    "deploy": "npm run build && wrangler pages publish ./build --project-name bolt-diy",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app",
    "lint:fix": "npm run lint -- --fix && prettier --write app",
    "typecheck": "tsc",
    "clean": "node scripts/clean.js",
    "prepare": "husky"
    // …electron and docker scripts left unchanged if you still need them
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "dependencies": {
    "@remix-run/cloudflare-pages": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    // …your other runtime deps
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "wrangler": "^3.108.0",
    "eslint": "^8.x",
    "vitest": "^2.x",
    // …your other build-time deps
  },
  "packageManager": "npm@9"
}
